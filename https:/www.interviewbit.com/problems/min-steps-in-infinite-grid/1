int Solution::coverPoints(vector<int> &A, vector<int> &B) {
    if(A.size()==1)
        return 0;
    int ans=0;
    for(int i=1;i<A.size();i++){
        int x1=A[i-1];
        int x2=A[i];
        int y1=B[i-1];
        int y2=B[i];
        int x=abs(x2-x1);
        int y=abs(y2-y1);
        ans+=std::max(x,y);
    }
    return ans;
        
}

